        "bookmarks.toggle"
      ]
    },
    {
      "before": [
        "<leader>",
        "b"
      ],
      "commands": [
        "bookmarks.list"
      ]
    }
  ],
  "vim.handleKeys": {
    "<C-a>": true,
    "<C-c>": false,
    "<C-d>": true,
    "<C-f>": true,
    "<C-h>": false,
    "<C-k>": false,
    "<C-r>": true,
    "<C-u>": false,
    "<C-v>": false,
    "<C-x>": true
  },

